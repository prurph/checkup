<%= render '/layouts/event_date_form' %>
<h1>Events</h1>

<button id="category-time-show" class="btn event-save-btn">Trace Your Time</button><br><br>
<span>Please enter the date period(mm/dd/yyyy)...</span><br>
<input id="start-event-input" placeholder="enter start time"/>
<input id="end-event-input" placeholder="enter end time"/><br>

<div class="user-data">
<ul id='category-time'>
  <li class='category-1'><ul class='category-1-tag'></ul></li>
  <li class='category-2'><ul class='category-2-tag'></ul></li>
  <li class='category-3'><ul class='category-3-tag'></ul></li>
  <li class='category-4'><ul class='category-4-tag'></ul></li>
</ul>
</div>
